  "private": true,
  "dockerRegistry": "docker-registry.internal-build:5000",
  "dockerRepo": "holey",
  "dependencies": {
    "amqplib": "^0.5.1",
    "assert": "^1.4.1",
    "body-parser": "^1.17.2",
    "bson": "^1.1.4",
    "co": "4.6.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-jwt": "^6.0.0",
    "express-validator": "^3.2.1",
    "graphql": "^0.10.3",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^2.2.30",
    "morgan": "^1.9.1",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "pjson": "^1.0.8",
    "request": "^2.83.0",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "crypto": "0.0.3",
    "docker-cmd": "^0.2.2",
    "localtunnel": "1.8.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "rimraf": "latest",
    "should": "^11.2.1"
  },
  "scripts": {
    "clean": "rimraf -rf dist",
    "compile": "babel app/ -d dist/app/",
    "compile:watch": "npm run compile -- -w",
    "build": "npm run clean && mkdir dist && cp -r bin public scripts package.json dist/ && npm run compile && node scripts/docker.js",
    "build-bamboo": "npm run clean && mkdir dist && cp -r bin public scripts package.json dist/ && npm run compile && node scripts/docker.js",
    "test": "mocha dist/**/*Spec.js",
    "test:watch": "onchange \"dist/**/*.js\" -- npm run test",
    "develop": "npm run clean; mkdir dist; sleep 3; npm-run-all -p compile:watch test:watch",
    "start": "node ./bin/www"
  }
}
